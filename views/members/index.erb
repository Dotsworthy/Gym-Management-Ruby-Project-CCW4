<h1>Gym Members</h1>
<a class="button" href="members/new">Add New Member</a>
<br>
<br>
<label for="">Search Member</label>
<input type="text" name="" value="">
<br>
<br>

<table class="table-container">
  <tr>
    <th>Username</th>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  <% for member in @members %>
  <tr>
    <td><a href="/members/<%= member.id%>"><%= member.user_name %></a></td>
    <td><%= member.first_name %></td>
    <td><%= member.last_name %></td>
  </tr>
  <% end %>
</table>
