<h1>Member Page</h1>
<h2>Member Actions</h2>
<h2>Member Details</h2>
<div>

  <form method="post" action ="/members/<%= @member.id %>/delete">
    <button class="delete" type="submit" value="Delete Member">Delete Member</button>
  </form>

  <form method="get" action ="/members/<%= @member.id %>/update">
    <button class="update" type="submit" value="Update Member">Update Member</button>
  </form>
</div>

<div class="detail-wrapper">
  Name: <%= @member.pretty_name %>
  Username: <%= @member.user_name %>
  <img class="profile" src="<%=@member.image_url%>.jpg" class="thumbnail" alt="Man">
</div>
