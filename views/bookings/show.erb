<p>Booking id: <%= @booking.id %>
<% @member = Member.find(@booking.member_id) %>
<%  @session = Session.find(@booking.session_id) %>
 -
 <%= @member.pretty_name %>
 -
 <%= @session.type %>
</p>

<form method="post" action ="/bookings/<%= @booking.id %>/delete">
  <button type="submit" value="Delete Booking">Delete Booking</button>
</form>

<form method="get" action ="/bookings/<%= @booking.id %>/update">
  <button type="submit" value="Update Booking">Update Booking</button>
</form>
