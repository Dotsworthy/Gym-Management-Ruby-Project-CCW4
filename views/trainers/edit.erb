<form method = "post" action ="/trainers/<%= @trainer.id %>">
  <label for = "first-name">First Name:</label>
    <input id= "first-name" type="test" name="first_name" value= "<%= @trainer.first_name %>"/>
<br>
  <label for= "last-name">Last Name:</label>
    <input id= "last-name" type="text" name="last_name" value= "<%= @trainer.last_name %>"/>
<br>
  <label for= "alias">Alias:</label>
    <input id= "alias" type="text" name="alias" value= "<%= @trainer.alias %>"/>
<br>
  <input type="submit" value="Update Trainer"/>
