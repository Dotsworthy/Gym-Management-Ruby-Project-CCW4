<h1>Gym Trainers</h1>
<h2>Trainer Search</h2>
<div class="member-title-wrapper">
  <h2>Trainer List</h2>
  <a class="button" href="trainers/new">Add Trainer</a>
  </div>
<div class="members-form-wrapper">
    <input class="search-bar" type="text" name="search" value="">
    <input class="search-image" type="image" src="images/search.png" alt="Submit">
  </div>
  <table class="table-container">
    <tr>
      <th>Username</th>
      <th>First Name</th>
    </tr>
<% for trainer in @trainers %>
  <tr>
    <td><a href="/trainers/<%= trainer.id%>">"<%= trainer.user_name %>"</a></td>
    <td><%= trainer.pretty_name %></td>
<% end %>
